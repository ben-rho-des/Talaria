{
  "name": "talaria",
  "version": "1.0.0",
  "description": "nodejs ts microservice",
  "main": "build/server.js",
  "author": "Ben Rhodes",
  "license": "MIT",
  "scripts": {
    "pm2": "pm2 start processes.json --no-daemon",
    "start": "npm run build && node ./build/server.js",
    "dev": "concurrently \"nodemon\" \"nodemon -x tsoa spec-and-routes\"",
    "lint": "tslint ./src/**/*.ts",
    "clean": "rimraf build",
    "build": "npm run clean && npm run lint && tsoa spec-and-routes && tsc",
    "tsoa": "tsoa"
  },
  "dependencies": {
    "@types/validator": "^13.6.3",
    "body-parser": "^1.15.2",
    "chalk": "^4.1.2",
    "express": "^4.14.0",
    "jest": "^27.0.6",
    "method-override": "^3.0.0",
    "pg": "^8.7.1",
    "sequelize": "^6.6.5",
    "sequelize-cli": "^6.2.0",
    "swagger-ui-express": "^4.1.6",
    "tsoa": "^3.9.0"
  },
  "devDependencies": {
    "@types/body-parser": "1.17.1",
    "@types/express": "^4.17.13",
    "@types/method-override": "0.0.31",
    "@types/node": "^16.7.1",
    "@types/swagger-ui-express": "^4.1.3",
    "concurrently": "^6.2.1",
    "ncp": "^2.0.0",
    "nodemon": "^2.0.12",
    "rimraf": "^3.0.0",
    "ts-node": "^10.2.1",
    "tslint": "^5.19.0",
    "typescript": "^4.3.5"
  }
}
